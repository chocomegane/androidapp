            android:label="@string/title_activity_tesuto"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".TestoActivity"
            android:label="@string/title_activity_testo"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".ImgActivity"></activity>
    </application>

</manifest>